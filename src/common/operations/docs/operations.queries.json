{
  "filter": {
    "name": "filter",
    "in": "query",
    "description": "Filter expression",
    "required": false,
    "schema": {
      "type": "string"
    }
  },
  "orderBy": {
    "name": "orderBy",
    "description": "Sort expression",
    "in": "query",
    "required": false,
    "schema": {
      "type": "string",
      "default": "asc"
    }
  },
  "limit": {
    "name": "limit",
    "description": "Max items returned",
    "in": "query",
    "required": false,
    "schema": {
      "type": "integer",
      "default": 25,
      "minimum": 1,
      "maximum": 200
    }
  },
  "skip": {
    "name": "skip",
    "in": "query",
    "description": "Items skipped",
    "required": false,
    "schema": {
      "type": "integer",
      "default": 0
    }
  }
}
